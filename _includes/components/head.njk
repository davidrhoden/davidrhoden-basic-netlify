<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {% set css %}
    {% include "assets/css/fixed-elements.css" %}
    {% include "assets/css/inline.css" %}
    {% include "assets/css/slideshow.css" %}
    {% include "assets/css/hamburgers.css" %}
    {% include "assets/css/svg-overlay.css" %}
    {% include "assets/css/magnific-popup.css" %}
    {% include "assets/css/fake-browser.css" %}
    {% include "assets/css/bricks.css" %}
    {% include "assets/css/timeline-two-sections.css" %}
    {% include "assets/css/modal-jq.css" %}
    {% include "static/webfonts/ShadowGrotesque/stylesheet.css" %}
    {% if davidrhodenbasicnetlify.environment == "development" %}
      {% include "assets/css/development.css" %}
    {% endif %}
  {% endset %}
  <style>
    {{ css | cssmin | safe }}
  </style>
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link href="https://fonts.googleapis.com/css2?family=Libre+Franklin:ital,wght@0,300;0,700;1,300;1,700&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Source+Serif+Pro:ital,wght@0,300;0,400;0,700;1,400&display=swap" rel="stylesheet">
  <link href="https://fonts.cdnfonts.com/css/cooper-black" rel="stylesheet">
  <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
  <link href="/pagefind/pagefind-ui.css" rel="stylesheet">
  <style>{{ css | safe }}</style>

  <script src="/pagefind/pagefind-ui.js"></script>
  <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.1/gsap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.1/CSSRulePlugin.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.1/MotionPathPlugin.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.1/ScrollTrigger.min.js"></script>
  {% set js %}
    {% include "assets/js/jquery.min.js" %}
    {% include "assets/js/jqModal.js" %}
    {% include "assets/js/jquery-cookie.js" %}
    {% include "assets/js/open-modal.js" %}
    {% include "assets/js/jquery.magnific-popup.min.js" %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | safe }}</script>
<script>
    window.addEventListener('DOMContentLoaded', (event) => {
        new PagefindUI({ element: "#search", showSubResults: true });
    });
</script>
  {% seo "" %}
</head>
